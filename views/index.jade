doctype html
html(lang="en")
    head
        title= 'Geo Data Visualization'
        meta(name='viewport' content='width=device-width, initial-scale=1')
        link(href='http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css', rel='stylesheet')
        link(href='style/style.css', rel='stylesheet')
    body
        .container-fluid
            h1#head= 'Geo Data Visualization'
            nav.navbar.navbar-defualt.navbar-inverse.navbar-fixed-top
                ul.nav.navbar-nav
                    li
                        a(href='#') Home
            .row#pre1
                .col-xs-12
                    br
            .row#chartContainer
                .col-xs-1#menu1a
                .col-xs-10.chart#map1
                .col-xs-1#menu1b
    block scripts
        script(src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
        script(src='http://cdnjs.cloudflare.com/ajax/libs/d3/3.5.3/d3.min.js')
        script(src='http://cdnjs.cloudflare.com/ajax/libs/lodash.js/3.1.0/lodash.min.js')
        script(src='http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js')
        script(src='http://cdnjs.cloudflare.com/ajax/libs/queue-async/1.0.7/queue.min.js')
        script(src='lib/d3-legend.min.js')
        script(src='lib/saveSvgAsPng.js')
        script(type = 'text/javascript').
            var INPUTS = {};
            INPUTS.states = (!{JSON.stringify(locals.locals)}).states;
        script(type = 'text/javascript').
            INPUTS.regions = (!{JSON.stringify(locals.locals)}).regions;
        script(src='lib/script.js')
            